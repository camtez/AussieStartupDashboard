{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://notion.so https://*.notion.so https://*.notion.site;"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://notion.so/"
        }
      ]
    }
  ]
}
