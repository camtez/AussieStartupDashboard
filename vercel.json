{
  "version": 2,
  "public": true,
  "builds": [{ "src": "index.html", "use": "@vercel/static" }],
  "routes": [{ "src": "/(.*)", "dest": "/public/$1" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://notion.so https://*.notion.so;"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://notion.so/"
        }
      ]
    }
  ]
}
